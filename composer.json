  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Ricardo Tapia",
      "email": "rtapia92@gmail.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "~6.0"
